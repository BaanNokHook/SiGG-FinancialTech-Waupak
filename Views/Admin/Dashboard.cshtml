@model GM.Application.Web.Models.AdminViewModel
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/MainLayout.cshtml";
}

<section class="working-content" data-spy="scroll" data-offset="120">
    <div class="container-fluid container-fixed">
        <!-- PageTitle -->
        <div class="row row-fixed">
            <div class="col-lg-12 col-fixed">
                <div class="page-title">
                    <div class="breadcrumb-wrap">
                        <ol class="breadcrumb">
                            <li>
                                <a href="..\Home\Index">Home</a>
                            </li>
                            <li class="active">Admin</li>
                            <li class="active">Rerun Job</li>
                        </ol>
                    </div>
                    <div class="title">
                        <h1>
                            RERUN JOB
                        </h1>
                    </div>
                </div>
            </div>
        </div>
        <!-- Content Zone -->
        <div class="row row-fixed">
            <div class="col-xs-12 col-fixed">

                <div id="alert-msg"></div>

                <!-- Div Detail -->
                <div class="box-content">
                    <div class="form-container form-horizontal">
                        <div class="row">
                            <div class="col-xs-12 form-wrapper-950 form-center-fix">
                                <div class="form-group">
                                    @Html.LabelFor(model => model.asof_date, htmlAttributes: new { @class = "control-label col-xs-2" })
                                    <div class="col-xs-4">
                                        <div class="input-group date input-range">
                                            @Html.EditDateFor(model => model.asof_date, new { @class = "form-control date-time-picker" })
                                            <span class="input-group-addon default date-pick-btn">
                                                <span class="feather-icon icon-calendar"></span>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <hr>
                        </div>
                        <div class="col-xs-12 form-wrapper-950 form-center-fix">
                            <div class="col-xs-12 form-sub-title col-form-fix">
                                <h4>Program Run</h4>
                            </div>
                            <div class="row margin-bottom-15">
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("ExportDWH_FCY_DailyToSFTP", "Admin")')">DWH(FCY-Daily)-SFTP</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("ExportDWH_FCY_MonthlyToSFTP", "Admin")')">DWH(FCY-Monthly)-SFTP</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("ExportDMSDataSetToSFTP", "Admin")')">DMS(DataSet-Daily)-SFTP</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("ExportDMSDataSetMonthlyToSFTP", "Admin")')">DMS(DataSet-Monthly)-SFTP</button>
                                </div>
                            </div>
                            <div class="row margin-bottom-15">
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("ExportDWH_THB_DailyToSFTP", "Admin")')">DWH(THB-Daily)-SFTP</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("ExportDWH_THB_MonthlyToSFTP", "Admin")')">DWH(THB-Monthly)-SFTP</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InterfaceCRTransLimitDaily", "Admin")')">CR(TransLimit-Daily)</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InterfaceCRTransLimitEod", "Admin")')">CR(TransLimit-Eod)</button>
                                </div>
                            </div>
                            <div class="row margin-bottom-15">
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("ExportFITSTrpToSFTP", "Admin")')">FITS(Trp)-SFTP</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InterfaceFITSBondPledge", "Admin")')">FITS(BondPledge)</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InterfaceSSMDExchangeRate", "Admin")')">GMX(ExchangeRate-Spot)</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InterfaceSSMDFloatingIndex", "Admin")')">GMX(FloatingIndexRate)</button>
                                </div>
                            </div>
                            <div class="row margin-bottom-15">
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InterfaceCCM", "Admin")')">CCM</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InterfaceMarketPriceBBG", "Admin")')">FITS(BBG-MarketPrice)</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("ExportAmendCancelDaily", "Admin")')">Export(AmendCancel-Daily)</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("ExportAmendCancelMonthly", "Admin")')">Export(AmendCancel-Monthly)</button>
                                </div>
                            </div>
                            <div class="row margin-bottom-15">
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InterfaceEXRATECounterRate", "Admin")')">EXRATE(CounterRate)</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InterfaceEXRATEMidRate", "Admin")')">EXRATE(MidRate)</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InterfaceValuationRate", "Admin")')">EXRATE(ValuationRate)</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("ExportUserProfileMonthly", "Admin")')">Export(UserProfile-Monthly)</button>
                                </div>
                            </div>
                            <div class="row margin-bottom-15">
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("ExportGLToSFTP", "Admin")')">GL-SFTP</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("ExportFxReconcileToSFTP", "Admin")')">Fx(Reconcile)-SFTP</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("ExportNesToSFTP", "Admin")')">NES-SFTP</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InterfaceThorRate", "Admin")')">GMX(Interface-THOR Rate)</button>
                                </div>
                            </div>
                            <div class="row margin-bottom-15">
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InterfaceMarketPriceTbmaT1", "Admin")')">FITS(TBMA-MarketPrice-T0)</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InterfaceMarketPriceTbmaT2", "Admin")')">FITS(TBMA-MarketPrice-T1)</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InterfaceEQUITYNavPrice", "Admin")')">EQUITY(NavPrice)</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InterfaceEQUITYPledge", "Admin")')">EQUITY(Pledge)</button>
                                </div>
                            </div>
                            <div class="row margin-bottom-15">
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InterfaceThorIndex", "Admin")')">FITS(Interface-THOR Index)</button>
                                </div>
                                <div class="col-xs-3">
                                </div>
                                <div class="col-xs-3">
                                </div>
                                <div class="col-xs-3">
                                </div>
                            </div>
                        </div>

                        <div class="col-xs-12 form-wrapper-950 form-center-fix">
                            <div class="col-xs-12 form-sub-title col-form-fix">
                                <h4>Internal Run</h4>
                            </div>
                            <div class="row margin-bottom-15">
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InternalBatchJobEod", "Admin")')">Internal BatchJobEod</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("InternalBatchJobEndOfDay", "Admin")')">Internal BatchJobEndOfDay</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="location.href='@Url.Action("Index", "SWIFT")';return false;">Internal SWIFT-SFTP</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="AdminDashboard.requestRerun('@Url.Action("CheckingEOD", "Admin")')">Checking EOD</button>
                                </div>
                            </div>
                            <div class="row margin-bottom-15">
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="location.href='@Url.Action("Index", "InternalPL")';return false;">Internal P&L</button>
                                </div>
                            </div>
                        </div>

                        <div class="col-xs-12 form-wrapper-950 form-center-fix">
                            <div class="col-xs-12 form-sub-title col-form-fix">
                                <h4>View</h4>
                            </div>
                            <div class="row margin-bottom-15">
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="location.href='@Url.Action("Index", "LogInOut")';return false;">LogInOut</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="location.href='@Url.Action("Index", "TransLeq")';return false;">TransLEQ</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="location.href='@Url.Action("Index", "DMS")';return false;">DMS(DataSet)</button>
                                </div>
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="location.href='@Url.Action("Index", "DWH")';return false;">DWH</button>
                                </div>
                            </div>
                            <div class="row margin-bottom-15">
                                <div class="col-xs-3">
                                    <button type="button" class="btn btn-primary btn-100w" onclick="location.href='@Url.Action("Index", "MarketPriceBBG")';return false;">BBG ( MarketPrice )</button>
                                </div>
                                <div class="col-xs-3">

                                </div>
                                <div class="col-xs-3">

                                </div>
                                <div class="col-xs-3">

                                </div>
                            </div>
                        </div>
                        @if (User.WizardPage == "Y")
                        {
                            <div class="col-xs-12 form-wrapper-950 form-center-fix">
                                <div class="col-xs-12 form-sub-title col-form-fix">
                                    <h4>Special</h4>
                                </div>
                                <div class="row margin-bottom-15">
                                    <div class="col-xs-3">
                                        <button type="button" class="btn btn-primary btn-100w" onclick="location.href='@Url.Action("WizardQuery", "Admin")'">Wizard Query</button>
                                    </div>
                                    <div class="col-xs-3">

                                    </div>
                                    <div class="col-xs-3">

                                    </div>
                                    <div class="col-xs-3">

                                    </div>
                                </div>
                            </div>
                        }
                        
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

@section Styles {
    @Styles.Render("~/bundles/SweetAlertCss")
    <style>
        .form-center-fix {
            float: none;
            margin: 0 auto;
        }

        .btn-100w {
            width: 100%;
        }
    </style>
}

@section Scripts {
    @Scripts.Render("~/bundles/SweetAlert")
    <script src="~/Scripts/GM.AdminDashboard.js">
    </script>
    
    <script>
        AdminDashboard.init();
    </script>

}