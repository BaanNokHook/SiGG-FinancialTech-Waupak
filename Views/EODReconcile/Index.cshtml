    <style>
        div.dataTables_wrapper {
            width: 100%;
            margin: 0 auto;
        }

        .dataTables_filter {
            display: none;
        }

        .dataTables_processing {
            z-index: 3000;
            padding-top: 10px !important;
        }

        .form-container .form-group .checkbox {
            padding-top: 5px;
        }

            .form-container .form-group .checkbox label {
                padding-left: 20px;
                margin-bottom: 0;
                font-weight: normal;
                padding-top: 0px;
            }

                .form-container .form-group .checkbox label input[type="checkbox"] {
                    margin: 2px 0;
                    margin-left: -20px;
                    cursor: pointer;
                }

                .form-container .form-group .checkbox label:after {
                    display: none;
                }

        /***** Toggle On-Off Switch Style *****/
        .onoffswitch {
            position: relative;
            width: 66px;
            -webkit-user-select: none;
            -moz-user-select: none;
            -ms-user-select: none;
        }

        .onoffswitch-checkbox {
            display: none;
        }

        .onoffswitch-label {
            display: block;
            overflow: hidden;
            cursor: pointer;
            border: 1px solid #cfdae7;
            border-radius: 50px;
            padding: 0 !important;
            float: none !important;
        }

            .onoffswitch-label:after {
                display: none !important;
            }

        .onoffswitch-inner {
            display: block;
            width: 200%;
            margin-left: -100%;
            -webkit-transition: margin 0.3s ease-in 0s;
            transition: margin 0.3s ease-in 0s;
        }

            .onoffswitch-inner:before, .onoffswitch-inner:after {
                display: block;
                float: left;
                width: 50%;
                height: 23px;
                padding: 0;
                line-height: 23px;
                font-size: 13px;
                color: white;
                font-weight: bold;
                -webkit-box-sizing: border-box;
                box-sizing: border-box;
            }

            .onoffswitch-inner:before {
                content: "YES";
                padding-left: 10px;
                background-color: #344961;
                color: #FFFFFF;
                text-align: left;
            }

            .onoffswitch-inner:after {
                content: "NO";
                padding-right: 10px;
                background-color: #F5F7FA;
                color: #5E8190;
                text-align: right;
            }

        .onoffswitch-switch {
            display: block;
            width: 15px;
            margin: 4px;
            background: #C1CCD1;
            position: absolute;
            top: 0;
            bottom: 0;
            right: 40px;
            border: 1px solid #C1CCD1;
            border-radius: 50px;
            -webkit-transition: all 0.3s ease-in 0s;
            transition: all 0.3s ease-in 0s;
        }

        .onoffswitch-checkbox:checked .onoffswitch-label .onoffswitch-inner {
            margin-left: 0;
        }

        .onoffswitch-checkbox:checked .onoffswitch-label .onoffswitch-switch {
            right: 0px;
            background: white;
            border-color: white;
        }

        .onoffswitch-checked .onoffswitch-inner {
            margin-left: 0;
        }

        .onoffswitch-checked .onoffswitch-switch {
            right: 0px;
        }


        .box-content-wrapper-d1 .box-content-d1 .form-container-d1 .form-group .checkbox {
            padding-top: 5px;
        }

            .box-content-wrapper-d1 .box-content-d1 .form-container-d1 .form-group .checkbox label {
                margin-bottom: 0;
                padding-top: 0px;
                padding-left: 20px;
            }

                .box-content-wrapper-d1 .box-content-d1 .form-container-d1 .form-group .checkbox label input[type="checkbox"] {
                    margin: 2px 0;
                    margin-left: -20px;
                }
    </style>
}

@section Scripts {
    @Scripts.Render("~/bundles/datatable")
    @Scripts.Render("~/bundles/SweetAlert")

    <script>
      $(document).ready(function () {
    $("#NavBar").html($('#NavEODReconcile').val());

          var urlDownloadEodBoReconcile = '@Url.Action("DownloadEodBoReconcile", "EODReconcile")';
          var urlDownloadEodReconcile = '@Url.Action("DownloadEodReconcile", "EODReconcile")';
          var urlDownloadEodReconcileCallMargin = '@Url.Action("DownloadEodReconcileCallMargin", "EODReconcile")';

    $('#btnRePrint').click(function () {

        var url = urlDownloadEodBoReconcile + '?asofDate=' + $('#asof_date').val();
        window.open(url, '_blank');
        return;
    });

    $('#btnPrint').click(function () {

        var boChecked = [], callMarginChecked = [];
        $("input[name='options[]']:checked").each(function () {
            var valChecked = $(this).val();
            if (valChecked == 'BILATERAL_MARGIN_PAY' || valChecked == 'PRIVATE_MARGIN_PAY')
                callMarginChecked.push(valChecked);
            else
                boChecked.push(valChecked);
        });

        var url;

        if (boChecked.length && !callMarginChecked.length) {
            url = urlDownloadEodReconcile + '?asofDate=' + $('#asof_date').val() + '&type=' + boChecked.join(',');
            window.open(url, '_blank');
            return;
        }
        else if (!boChecked.length && callMarginChecked.length) {
            url = urlDownloadEodReconcileCallMargin + '?asofDate=' + $('#asof_date').val() + '&type=' + callMarginChecked.join(',');
            window.open(url, '_blank');
            return;
        } else {
            swal("Warning", "Please Select One of Type Report", "error");
            return;
        }
    });

    $('#asof_date').on('dp.change', function (e) {
        Search();
    });
});
    </script>

    @*@Scripts.Render("~/module/EODReconcile")*@
    @Scripts.Render("~/module/validation")

    @Scripts.RenderFormat("<script type=\"text/javascript\" src=\"{0}?nocache=" + DateTime.Now.ToString("ddmmyyyyHHmmss") + "\"></script>", "~/module/EODReconcile")
}