                $('#ca_no').attr('disabled', 'disabled');
                $('[name=netting_flag]').attr('disabled', 'disabled');
                $('[name=default_flag]').attr('disabled', 'disabled');
            }

            //$("#maxtest").keypress(function () {
            //    alert($('#maxtest').val());
            //});


            $("#btnIdentifySave").click(function () {
                var guarantorpercent = $('#IdentifyRightModal_identify_no').val();
                var input = $('#IdentifyRightModal_identify_no');
                if (guarantorpercent == "") {
                    $("#identify_no_error").text("The Identify No field is required.");
                    input.addClass("input-validation-error");
                }
            });

            //set date start end
            var openDateId = $("#open_date");
            var closeDateId = $("#close_date");
            if (openDateId.val().length > 0) {
                closeDateId.data("DateTimePicker").minDate(openDateId.val());
            }

            if (closeDateId.val().length > 0) {
                openDateId.data("DateTimePicker").maxDate(closeDateId.val());
            }
            
            openDateId.on("dp.change", function (e) {
                closeDateId.data("DateTimePicker").minDate(e.date);
            });


            closeDateId.on("dp.change", function (e) {
                openDateId.data("DateTimePicker").maxDate(e.date);
            });


            $("#NavBar").html($('#NavIssuer').val());
        });
    </script>
