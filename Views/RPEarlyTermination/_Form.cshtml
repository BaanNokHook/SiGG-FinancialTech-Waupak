                                                        <span class="selected-data">@Model.trans_deal_type_name</span>
                                                        @Html.HiddenFor(model => model.trans_deal_type)
                                                        <span class="caret"></span>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-xs-4 col-md-5 col-lg-4">
                                            </div>
                                            <div class="col-xs-5 col-md-7 col-lg-8">
                                            </div>
                                        </div>
                                        <div class="form-group">
