                                            <div class="col-xs-5 col-md-7 col-lg-8">
                                                <div class="input-group no-btn" style="max-width: 250px;width: 100%;">
                                                    <input id="interest_total_int" readonly type="sub-desk" class="form-control" placeholder="Input numbers only" value="@(Model.interest_rate != null && Model.interest_spread != null ? (Model.interest_rate + Model.interest_spread).Value.ToString("N8") : "")">
                                                    <span disabled class="input-group-addon">
                                                        <span class="text">%</span>
                                                        @Html.HiddenFor(model => model.interest_total)
